@model GCETNChapter.Models.ViewModels.DonationDetailsVO
<script src="~/JQueryScripts/Donations/Donations.js"></script>

@{
    ViewBag.Title = "Donatoins";
}

<div class="row" style="height: 100%; width: 100%; display: block;">
    <div class="col-md-1"></div>
    <div id="divDonationDetailsPage" class="form-horizontal partialBodySection hideDiv col-md-10">
        <br />

        <div>
            @Html.Partial("_SuccessErrorMessages")
        </div>

        <div id="divViewAllDonations">
            @Html.Partial("_ViewDonations", Model)
        </div>

        <div id="divAddDonations" class="hideDiv">
            @Html.Partial("_AddDonations", Model)
        </div>

    </div>
    <div class="col-md-1"></div>
</div>


